{
  "directories": {
    "logging_directory": "./logs/",
    "output_directory": "./"
  },
  "execution_control": {
    "allocation": "rev",
    "feature": "--qos=high",
    "memory": 90,
    "nodes": 20,
    "option": "eagle",
    "sites_per_worker": 100,
    "walltime": 4.0
  },
  "project_control": {
    "analysis_years": [
      2011,
      2012,
      2013
    ],
    "logging_level": "INFO",
    "output_request": [
      "cf_mean",
      "cf_profile",
      "lcoe_fcr",
      "ghi_mean"
    ],
    "technology": "pv"
  },
  "project_points": "./nsrdb_conus_project_points.csv",
  "resource_file": "/datasets/NSRDB/v3.0.1/nsrdb_{}.h5",
  "sam_files": {
    "default": "./pv.json"
  }
}
